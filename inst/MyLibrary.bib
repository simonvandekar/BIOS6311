	author = {Brookhart, M. Alan and van der Laan, Mark J.},
	month = jan,
	year = {2006},
	note = {Number: 2},
	keywords = {Causal inference, Censored data, Cross validation, Model selection, Nuisance parameter, Semiparametric model},
	pages = {475--498},
}

@article{linde_bayesian_2012-1,
