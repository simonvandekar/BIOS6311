	keywords = {Random field theory, Cluster mass, Gaussian field, Gaussianized  image},
	pages = {51--61},
	file = {Accepted Version:/Users/vandeks/Zotero/storage/2NW5WXYZ/Zhang et al. - 2009 - Cluster mass inference via random field theory.pdf:application/pdf;ScienceDirect Snapshot:/Users/vandeks/Zotero/storage/QZX6PFLI/S1053811908009117.html:text/html},
}

