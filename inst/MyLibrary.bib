	journal = {bioRxiv},
	author = {Chen, Andrew A. and Beer, Joanne C. and Tustison, Nicholas J. and Cook, Philip A. and Shinohara, Russell T. and Shou, Haochang and Initiative, for the Alzheimer’s Disease Neuroimaging},
