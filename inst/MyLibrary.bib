	keywords = {Cluster-robust variance estimator, Clustered data, Difference-in-differences, Grouped data, Robust inference, Subclustering, Treatment model, Wild bootstrap, Wild cluster bootstrap},
	pages = {114--135},
	file = {Snapshot:/Users/vandeks/Zotero/storage/9Y9IBFL2/ectj.html:text/html;Submitted Version:/Users/vandeks/Zotero/storage/UJBM9L4V/MacKinnon and Webb - 2018 - The wild bootstrap for few (treated) clusters.pdf:application/pdf},
