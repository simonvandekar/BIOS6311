	author = {Hájek, Alan},
	month = jun,
	year = {2007},
	keywords = {Classical, Conditional probability, Frequentist, Kolmogorov, Logical, Popper, Probability, Propensity, Reference class problem, Subjectivist interpretations of probability},
	pages = {563--585},
	file = {Springer Full Text PDF:/Users/vandeks/Zotero/storage/Z47MP4Z2/Hájek - 2007 - The reference class problem is your problem too.pdf:application/pdf},
