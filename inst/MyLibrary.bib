	pages = {535--540},
	file = {Full Text PDF:/Users/vandeks/Zotero/storage/8RQYKGEC/Kriegeskorte et al. - 2009 - Circular analysis in systems neuroscience the dan.pdf:application/pdf;Snapshot:/Users/vandeks/Zotero/storage/D82H67HI/nn.html:text/html},
}

