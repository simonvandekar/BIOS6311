	journal = {Biological Psychiatry: Cognitive Neuroscience and Neuroimaging},
	author = {Korgaonkar, Mayuresh S. and Erlinger, May and Breukelaar, Isabella A. and Boyce, Philip and Hazell, Philip and Antees, Cassandra and Foster, Sheryl and Grieve, Stuart M. and Gomes, Lavier and Williams, Leanne M. and Harris, Anthony W. F. and Malhi, Gin S.},
	month = aug,
	year = {2018},
	keywords = {amygdala, bipolar disorders, depression, emotion processing, fMRI connectivity, remission},
