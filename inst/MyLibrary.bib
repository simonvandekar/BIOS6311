@article{schwartzman_peak_2019-1,
	title = {Peak p-values and false discovery rate inference in neuroimaging},
	volume = {197},
	issn = {1053-8119},
	url = {https://www.sciencedirect.com/science/article/pii/S1053811919303271},
	doi = {10.1016/j.neuroimage.2019.04.041},
	abstract = {Peaks are a mainstay of neuroimage analysis for reporting localization results. The current peak detection procedure in SPM12 requires a pre-threshold for approximating p-values and a false discovery rate (FDR) nominal level for inference. However, the pre-threshold is an undesirable feature, while the FDR level is meaningless if the null hypothesis is not properly defined. This article provides: 1) a peak height distribution for smooth Gaussian error fields, which does not require a screening pre-threshold; 2) a signal-plus-noise model where FDR of peaks can be controlled and properly interpreted. Matlab code for calculation of p-values using the exact peak height distribution is available as an SPM extension.},
