	month = mar,
	year = {2014},
	pmid = {24565361},
	pmcid = {PMC3935178},
	note = {Number: 3},
	keywords = {Adolescent, Adult, Female, Humans, Young Adult, development, Magnetic Resonance Imaging, Amygdala, hippocampus, Hippocampus, magnetic resonance imaging (MRI), Male, puberty, Puberty, Sex Factors},
	pages = {341--350.e1},
}

