	month = may,
	year = {2019},
	note = {Publication Title: Reproducibility and Replicability in Science},
	file = {Snapshot:/Users/vandeks/Zotero/storage/XSDTKW9W/NBK547524.html:text/html},
}

@article{tao_optimal_2020,
	title = {Optimal {Designs} of {Two}-{Phase} {Studies}},
	volume = {115},
	issn = {0162-1459},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC7954143/},
	doi = {10.1080/01621459.2019.1671200},
	abstract = {The two-phase design is a cost-effective sampling strategy to evaluate the effects of covariates on an outcome when certain covariates are too expensive to be measured on all study subjects. Under such a design, the outcome and inexpensive covariates are measured on all subjects in the first phase and the first-phase information is used to select subjects for measurements of expensive covariates in the second phase. Previous research on two-phase studies has focused largely on the inference procedures rather than the design aspects. We investigate the design efficiency of the two-phase study, as measured by the semiparametric efficiency bound for estimating the regression coefficients of expensive covariates. We consider general two-phase studies, where the outcome variable can be continuous, discrete, or censored, and the second-phase sampling can depend on the first-phase data in any manner. We develop optimal or approximately optimal two-phase designs, which can be substantially more efficient than the existing designs. We demonstrate the improvements of the new designs over the existing ones through extensive simulation studies and two large medical studies.},
	number = {532},
	urldate = {2023-04-17},
	journal = {Journal of the American Statistical Association},
	author = {Tao, Ran and Zeng, Donglin and Lin, Dan-Yu},
	year = {2020},
	pmid = {33716361},
	pmcid = {PMC7954143},
	pages = {1946--1959},
	file = {PubMed Central Full Text PDF:/Users/vandeks/Zotero/storage/HI4AH8JV/Tao et al. - 2020 - Optimal Designs of Two-Phase Studies.pdf:application/pdf},
}

@article{schildcrout_outcome_2013,
