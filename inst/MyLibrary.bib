	journal = {Biometrics},
	author = {Schildcrout, Jonathan S. and Garbett, Shawn P. and Heagerty, Patrick J.},
	year = {2013},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/biom.12013},
	keywords = {Asthma, Biased sampling, Linear mixed effects models, Longitudinal data analysis, Outcome dependent sampling, Time-dependent covariates},
	pages = {405--416},
	file = {Full Text PDF:/Users/vandeks/Zotero/storage/YLD4DBY4/Schildcrout et al. - 2013 - Outcome Vector Dependent Sampling with Longitudina.pdf:application/pdf;Snapshot:/Users/vandeks/Zotero/storage/IUMWC2CF/biom.html:text/html},
}

@article{tao_efficient_2017,
	title = {Efficient {Semiparametric} {Inference} {Under} {Two}-{Phase} {Sampling}, {With} {Applications} to {Genetic} {Association} {Studies}},
	volume = {112},
	issn = {0162-1459},
	doi = {10.1080/01621459.2017.1295864},
	abstract = {In modern epidemiological and clinical studies, the covariates of interest may involve genome sequencing, biomarker assay, or medical imaging and thus are prohibitively expensive to measure on a large number of subjects. A cost-effective solution is the two-phase design, under which the outcome and inexpensive covariates are observed for all subjects during the first phase and that information is used to select subjects for measurements of expensive covariates during the second phase. For example, subjects with extreme values of quantitative traits were selected for whole-exome sequencing in the National Heart, Lung, and Blood Institute (NHLBI) Exome Sequencing Project (ESP). Herein, we consider general two-phase designs, where the outcome can be continuous or discrete, and inexpensive covariates can be continuous and correlated with expensive covariates. We propose a semiparametric approach to regression analysis by approximating the conditional density functions of expensive covariates given inexpensive covariates with B-spline sieves. We devise a computationally efficient and numerically stable EM-algorithm to maximize the sieve likelihood. In addition, we establish the consistency, asymptotic normality, and asymptotic efficiency of the estimators. Furthermore, we demonstrate the superiority of the proposed methods over existing ones through extensive simulation studies. Finally, we present applications to the aforementioned NHLBI ESP.},
	language = {eng},
	number = {520},
	journal = {Journal of the American Statistical Association},
	author = {Tao, Ran and Zeng, Donglin and Lin, Dan-Yu},
	year = {2017},
	pmid = {29479125},
	pmcid = {PMC5823539},
	keywords = {EM algorithm, Biased sampling, Genome sequencing, Responseselective sampling, Semiparametric efficiency, Sieve approximation},
	pages = {1468--1476},
	file = {Accepted Version:/Users/vandeks/Zotero/storage/VEI22KKW/Tao et al. - 2017 - Efficient Semiparametric Inference Under Two-Phase.pdf:application/pdf},
}

@article{nielsen_machine_2020,
	series = {Understanding the {Nature} and {Treatment} of {Psychopathology}: {Letting} the {Data} {Guide} the {Way}},
	title = {Machine {Learning} {With} {Neuroimaging}: {Evaluating} {Its} {Applications} in {Psychiatry}},
	volume = {5},
	issn = {2451-9022},
