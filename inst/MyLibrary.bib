	year = {2014},
	keywords = {magnetic resonance imaging, Brain imaging, diffusion MRI, machine learning, segmentation, thalamus parcellation},
	pages = {169--176},
	file = {Springer Full Text PDF:/Users/vandeks/Zotero/storage/EBX6ZFTE/Stough et al. - 2014 - Automatic Method for Thalamus Parcellation Using M.pdf:application/pdf},
