	journal = {Sociological Methodology},
	author = {Von Hippel, Paul T.},
	year = {2009},
	note = {Number: 1
\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1467-9531.2009.01215.x},
	pages = {265--291},
}

@article{bodner_what_2008-1,
