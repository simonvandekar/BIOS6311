	number = {4},
	urldate = {2019-04-03},
	journal = {Cognitive, Affective, \& Behavioral Neuroscience},
	author = {Shou, H. and Eloyan, A. and Lee, S. and Zipunnikov, V. and Crainiceanu, A. N. and Nebel, M. B. and Caffo, B. and Lindquist, M. A. and Crainiceanu, C. M.},
