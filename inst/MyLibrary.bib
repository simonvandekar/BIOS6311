	pmid = {30545854},
	pages = {eaat7615},
	file = {Full Text PDF:/Users/vandeks/Zotero/storage/SK62D7PF/Li et al. - 2018 - Integrative functional genomic analysis of human b.pdf:application/pdf;Snapshot:/Users/vandeks/Zotero/storage/SMAPIDHJ/eaat7615.html:text/html},
}

