	urldate = {2023-01-12},
	journal = {Journal of Autism and Developmental Disorders},
	author = {Smith, Joshua R. and Hopkins, Corey E. and Xiong, Jiangmei and Luccarelli, James and Shultz, Elizabeth and Vandekar, Simon},
