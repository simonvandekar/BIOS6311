	keywords = {Research Design, Artifacts, High-Throughput Screening Assays, Microarray Analysis},
	pages = {4934--4940},
	file = {Full Text:/Users/vandeks/Zotero/storage/P93GTAA2/Hunt et al. - 2022 - Systematic replication enables normalization of hi.pdf:application/pdf},
